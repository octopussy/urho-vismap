<renderpath>
    <rendertarget name="RTVismap" sizedivisor="1 1" filter="true"/>

    <command type="clear" color="0.1 0.1 0.1 1" output="RTVismap"/>

    <command type="scenepass" pass="vismap" output="RTVismap"/>

    <command type="clear" color="fog" depth="1.0" stencil="0"/>
    <command type="scenepass" pass="base" vertexlights="true" metadata="base"/>

    <command type="quad" vs="CopyFramebuffer" ps="CopyFramebuffer" output="viewport">
        <texture unit="diffuse" name="RTVismap"/>
    </command>

    <!--command type="quad" vs="VisMap_apply" ps="VisMap_apply" output="viewport">
